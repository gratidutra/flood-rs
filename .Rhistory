library(prophet)
library(tidyverse)
df <- read_csv('data/data.csv') %>%
filter(Rio == "s\xe3o gon\xe7alo")
df$ds <- lubridate::dmy_hms(df$ds)
head(df)
m <- prophet(df, growth = 'linear', yearly.seasonality=F)
future <- make_future_dataframe(m, periods = 300, freq = 60 * 60)
fcst <- predict(m, future)
plot(m, fcst)
tail(future)
view(fcst[c('ds', 'yhat', 'yhat_lower', 'yhat_upper')])
